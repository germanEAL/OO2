package ar.edu.unlp.info.oo2.Ejercicio3P3;

public class Dispositivo {
	private Connection conect;
	private Ringer ringRing;
	private CrcCalculator crc;
	private Display dis;
	public Dispositivo() {
		this.conect = new Connection();
		this.ringRing = new Ringer();
		this.crc = crc;
		this.dis = dis;
	}
	
	
}
